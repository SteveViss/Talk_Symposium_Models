\definecolor{foreground}{RGB}{105,104,101}
\definecolor{text}{RGB}{25,24,21}
\definecolor{uqarblue}{RGB}{40,102,151}
\definecolor{uqarred}{RGB}{213,26,35}
\definecolor{uqargreen}{RGB}{102,160,10}

\setbeamercolor{normal text}{fg=text}
\setbeamercolor{frametitle}{fg=foreground}
\setbeamercolor{framesubtitle}{fg=uqarblue}

\setbeamercolor{title}{fg=uqarblue}
\setbeamercolor{subtitle}{fg=foreground}

\setbeamercolor{alerted text}{fg=uqarblue}
\setbeamercolor{structure}{fg=uqarblue}

\setbeamercolor{footlinecolor}{fg=uqarblue}
\setbeamercolor{section in head/foot}{fg=foreground}

%%%%%%%%%%% Block color sets %%%%%%%%%%%%%%%%

% Standard block
\setbeamercolor{block title}{fg = uqarblue, bg = uqarblue!30}
\setbeamercolor{block body}{bg = uqarblue!30}

% Alert block
\setbeamercolor{block title alerted}{fg = uqarred, bg = uqarred!30}
\setbeamercolor{block body alerted}{ bg = uqarred!30}

% Example block
\setbeamercolor{block title example}{fg = uqargreen, bg = uqargreen!30}
\setbeamercolor{block body example}{ bg = uqargreen!30}

